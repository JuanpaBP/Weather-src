<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    <bean id="springDBConnection" class="Configuration.DB_Connect"/>
    <bean id="springPersistencyCountry" class="Persistency.PersistCountry"/>
    <bean id="springPersistencyState" class="Persistency.PersistState"/>
    <bean id="springPersistencyWeather" class="Persistency.PersistWeather"/>
</beans>